{
   "nixEnvPicker.envFile": "${workspaceFolder}${/}flake.nix",
   "nixEnvPicker.terminalAutoActivate": true,
   "nixEnvPicker.terminalActivateCommand": "exec nix develop",
   "clangd.path": "/usr/bin/env",
   "clangd.arguments": [
      "nix",
      "run",
      ".#clangd"
   ],
   "cmake.configureArgs": [
      "-DBAD_CURLPP_VERSION_FORMAT:BOOL=TRUE"
   ]
}